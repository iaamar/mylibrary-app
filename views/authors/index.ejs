<h2>All Authors</h2>
<form action="/authors" method="GET">
  <label>Name</label>
  <input type="text" name="name" value="<%= serachQuery.name %> " />
  <button type="submit">Search</button>
</form>
<% authors.forEach(author=> { %>
  <div>
    <%= author.name %>
  </div>
  <a href="/authors/<%= author.id %>">view</a>
  <a href="/authors/<%= author.id %>/edit">edit</a>
  <%- include('../partials/delete_form.ejs', { url:`/authors/${author.id}` }) %>
    <% }) %>